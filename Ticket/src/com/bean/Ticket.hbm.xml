<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping 	package="com.bean">
	<class name="Ticket" table="ticket">
		<id name="id" column="t_id" type="integer">
			<generator class="assigned"></generator>
		</id>
		<property name="start" column="t_start" not-null="true" />
		<property name="end" column="t_end" not-null="true" />
		<property name="price" column="t_price" not-null="true" />
		<property name="model" column="t_model" not-null="true" />
		<property name="ticketFactory" column="t_ticketFactory" not-null="true" />
		<property name="ticketnum" column="t_ticketnum" not-null="true" />
		<property name="flytime" column="flytime"  />
	</class>
	
</hibernate-mapping>